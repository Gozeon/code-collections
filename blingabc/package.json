{
  "name": "admin",
  "version": "0.4.7",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --hmr",
    "build:prod": "./node_modules/.bin/ng build --prod",
    "build:dev": "./node_modules/.bin/ng build --prod --environment=dev",
    "build": "npm run build:dev",
    "deploy:dev": "ng build --prod --environment=dev && npm run commit:dev",
    "deploy:prod": "ng build --prod && npm run commit:prod",
    "commit:dev": "git add dist/ && git commit -m 'update dist --> dev'",
    "commit:prod": "git add dist/ && git commit -m 'update dist --> prod'",
    "compress": "node scripts/command.js compress",
    "table": "node scripts/command.js table",
    "memory": "node scripts/command.js memory",
    "fix-memory-limit": "cross-env LIMIT=2048 increase-memory-limit",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "changelog": "./node_modules/.bin/conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "analyze": "ng build --prod --aot=false --stats-json; webpack-bundle-analyzer dist/stats.json",
    "postinstall": "npm run compress && npm run table"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.0",
    "@angular/cdk": "^5.2.0",
    "@angular/common": "^5.2.0",
    "@angular/compiler": "^5.2.0",
    "@angular/core": "^5.2.0",
    "@angular/forms": "^5.2.0",
    "@angular/http": "^5.2.0",
    "@angular/material": "^5.2.0",
    "@angular/platform-browser": "^5.2.0",
    "@angular/platform-browser-dynamic": "^5.2.0",
    "@angular/platform-server": "^5.2.0",
    "@angular/router": "^5.2.0",
    "@angular/material-moment-adapter": "^5.2.0",
    "@covalent/core": "^1.0.0-rc.0",
    "@covalent/dynamic-forms": "^1.0.0-rc.0",
    "@covalent/http": "^1.0.0-rc.0",
    "@ngrx/core": "^1.2.0",
    "@ngrx/store": "^4.1.1",
    "@ngrx/store-devtools": "^4.1.1",
    "chalk": "^2.3.0",
    "core-js": "^2.4.1",
    "hammerjs": "^2.0.8",
    "highlight.js": "9.11.0",
    "ng2-dragula": "1.5.0",
    "ngx-qrcode2": "0.1.0",
    "quill": "^1.3.4",
    "rxjs": "5.5.6",
    "web-animations-js": "2.3.1",
    "zone.js": "^0.8.17"
  },
  "devDependencies": {
    "@angular-devkit/core": "^0.4.5",
    "@angular/cli": "1.6.7",
    "@angular/compiler-cli": "^5.2.0",
    "@types/fs-extra": "^4.0.0",
    "@types/hammerjs": "^2.0.30",
    "@types/jasmine": "2.5.38",
    "@types/node": "~6.0.60",
    "@types/selenium-webdriver": "^2.52.0",
    "autoprefixer": "7.1.2",
    "codelyzer": "~4.0.0",
    "compression-webpack-plugin": "^1.0.1",
    "conventional-changelog-cli": "1.3.3",
    "glob": "^6.0.4",
    "jasmine-core": "~2.5.2",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.3.0",
    "karma-firefox-launcher": "^1.0.1",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-phantomjs-launcher": "1.0.1",
    "moment": "2.18.1",
    "node-sass": "^4.7.2",
    "protractor": "~5.1.0",
    "showdown": "^1.8.5",
    "ts-node": "^3.0.4",
    "tslint": "5.2.0",
    "typescript": "~2.6.2",
    "webpack-bundle-analyzer": "^2.9.1"
  }
}
